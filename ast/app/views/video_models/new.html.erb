<h1>New video_model</h1>

<%= error_messages_for :video_model %>

<% form_for(@video_model) do |f| %>
  <p>
    <b>Manufacture</b><br />
    <%= f.text_field :manufacture %>
  </p>

  <p>
    <b>Model</b><br />
    <%= f.text_field :model %>
  </p>


  <p>
    <%= f.submit "Create" %>
  </p>
<% end %>

<%= link_to 'Back', video_models_path %>
