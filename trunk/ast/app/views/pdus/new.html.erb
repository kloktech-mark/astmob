<h1>New pdu</h1>

<%= error_messages_for :pdu %>

<% form_for(@pdu) do |f| %>

  <%= render (:partial => "assets/mini_new", :object => @pdu.asset) %>

  <p>
    <b>Pdu Model</b><br />
    <%= f.collection_select("pdu_model_id", PduModel.find(:all), "id", "part_number") %>
  </p>
<br/>
  <p>
    <%= f.submit "Create" %>
  </p>
<% end %>

<%= link_to 'Back', pdus_path %>
